<div class="container">
  <p id="notice"><%= notice %></p>

  <p>
    <strong>Name:</strong>
    <%= @user.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= @user.email %>
  </p>

  <p>
    <strong>Provider:</strong>
    <%= @user.provider %>
  </p>

  <p>
    <strong>Uid:</strong>
    <%= @user.uid %>
  </p>

  <%= link_to 'Edit', edit_user_path(@user) if can_update?(@user) %> |
  <%= link_to 'Back', users_path %>
</div>
